{
    "name": "fcm-clone-server",
    "version": "1.0.0",
    "description": "FCM-style notification platform backend",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "db:setup": "node -r ts-node/register src/scripts/setupDb.ts"
    },
    "keywords": [
        "push",
        "notifications",
        "fcm"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "bcrypt": "^5.1.1",
        "jsonwebtoken": "^9.0.2",
        "web-push": "^3.6.6",
        "dotenv": "^16.3.1",
        "cors": "^2.8.5",
        "express-rate-limit": "^7.1.5",
        "nanoid": "^3.3.7"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.10.9",
        "@types/bcrypt": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/web-push": "^3.6.3",
        "@types/cors": "^2.8.17",
        "typescript": "^5.3.3",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0"
    }
}